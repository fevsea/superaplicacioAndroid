<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pr_idi.mydatabaseexample"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="17"
        android:targetSdkVersion="24" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.pr_idi.mydatabaseexample.PrincipalActivity"
            android:label="@string/title_activity_principal"
            android:theme="@style/AppTheme.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.pr_idi.mydatabaseexample.AddItem" />
        <activity android:name="com.example.pr_idi.mydatabaseexample.ViewItem" />
        <activity
            android:name="com.example.pr_idi.mydatabaseexample.About"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.example.pr_idi.mydatabaseexample.Help"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>